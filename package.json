{
  "name": "create-component-template",
  "version": "1.0.0",
  "description": "Generates react components from templates",
  "main": "./dist/create-component-template.js",
  "bin": {
    "create-component": "./dist/create-component-template"
  },
  "files": [
    "./dist/*",
    "./templates/*"
  ],
  "keywords": [
    "Create",
    "Generate",
    "React",
    "Component",
    "Library",
    "Monorepo"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-watch": "^2.0.7",
    "exit-hook": "^2.0.0",
    "flow-bin": "^0.69.0",
    "flow-remove-types": "^1.2.3",
    "husky": "^0.14.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "jest": "^22.4.2",
    "mkdirp-promise": "^5.0.1",
    "pkg-dir": "^2.0.0",
    "prompt": "^1.0.0",
    "semver-regex": "^1.0.0",
    "yargs": "^11.0.0"
  },
  "scripts": {
    "test": "jest --watch",
    "start": "npm run build && npm run start-dist",
    "start-dist": "node ./dist/create-component-template",
    "build": "babel ./src/ -d ./dist/ --all --pretty",
    "prepush": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyPickard/react-component-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonnyPickard/react-component-generator/issues"
  },
  "homepage": "https://github.com/JonnyPickard/react-component-generator#readme"
}
